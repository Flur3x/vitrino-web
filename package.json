{
  "name": "vitrino",
  "version": "0.0.1",
  "description": "A shopping app for local stores",
  "engines": {
    "node": "6.2.2",
    "npm": "3.9.5"
  },
  "main": "app.js",
  "scripts": {
    "postinstall": "npm run build",
    "watchify": "watchify -vd -p browserify-hmr -e client/main.js -o public/js/vitrino.js",
    "serve": "http-server -o -c 1 -a localhost",
    "node": "node app.js",
    "dev": "npm-run-all --parallel watchify node",
    "lint": "eslint --ext .js,.vue src test/unit",
    "test": "karma start karma.conf.js",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o public/css/vitrino.css ] -e client/main.js | uglifyjs -c warnings=false -m > public/js/vitrino.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flur3x/Vitrino.git"
  },
  "author": "Daniel Bischoff",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Flur3x/Vitrino/issues"
  },
  "homepage": "https://github.com/Flur3x/Vitrino#readme",
  "dependencies": {
    "autoprefixer": "^6.5.2",
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.2",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cheerio": "^0.22.0",
    "connect-flash": "^0.1.1",
    "connect-history-api-fallback": "^1.3.0",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^2.0.0",
    "csurf": "^1.9.0",
    "envify": "^3.4.1",
    "eslint": "^3.3.0",
    "eslint-config-google": "^0.6.0",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-cssmin": "^1.0.2",
    "grunt-contrib-less": "^1.3.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-jsdoc": "^2.1.0",
    "grunt-mocha-test": "^0.13.2",
    "grunt-nunjucks": "^0.1.4",
    "grunt-postcss": "^0.8.0",
    "helmet": "^2.1.2",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-browserify": "^5.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "knex": "^0.12.2",
    "less": "^2.7.1",
    "mocha": "^3.0.2",
    "morgan": "^1.7.0",
    "npm-run-all": "^2.3.0",
    "nunjucks": "^2.4.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "6.1.0",
    "phantomjs-prebuilt": "^2.1.3",
    "proxyquireify": "^3.0.1",
    "raven-js": "^3.9.1",
    "sendgrid": "^4.0.2",
    "supertest": "^2.0.0",
    "uglify-js": "^2.5.0",
    "vue": "^2.0.1",
    "vue-router": "^2.1.1",
    "vueify": "^9.4.0",
    "watchify": "^3.4.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
